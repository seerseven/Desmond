
  
  <style>

        #siteBrand{
            width: 220px;
   
        position: relative;
        }
        #logo-trans {
            width: 100%;
        height: 100%;            
        position: absolute;
        top: 0;
        left: 0;
        }
        #logo-dark {
            z-index: 9;
        }
        #logo-light {
            z-index: 9;
        }
  </style>
  <div id="siteBrand" class="flxr-s">
    
        <div class="logo">
            {%- if section.settings.enableTransparentHeader != blank and request.page_type == 'index' -%}
            <img id="logo-trans" class="d-block wp-{{ logoSize }} min-wx-{{ logoMinWidth }}" src="{{ logoDark | img_url: 'master' }}"   >
            {%- endif -%}
            <img id="logo-light" class="d-block wp-{{ logoSize }} min-wx-{{ logoMinWidth }}" src="{{ logoLight | img_url: 'master' }}"   >
            <img id="logo-dark" class=" d-none wp-{{ logoSize }} min-wx-{{ logoMinWidth }}" src="{{ logoDark | img_url: 'master' }}"   >
            
        </div>
  
  </div>

